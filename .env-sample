# Spaced Repetition App Configuration
# Copy this file to .env and fill in your values

# Z.AI API Configuration
# Get your API key from: https://z.ai/manage-apikey/apikey-list
ZAI_API_KEY=your-api-key-here

# API base URL: "default" or "coding" (for coding-specific endpoint)
# ZAI_BASE_URL=default

# Enable/disable API evaluation (set to "false" to always use clipboard)
# ZAI_ENABLED=true

# Enable/disable clipboard fallback when API fails
# USE_CLIPBOARD_FALLBACK=true
